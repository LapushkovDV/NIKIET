     and ((SaldTune.WKAU[5] = 0) or (SaldTune.WKAU[5] <> 0 and spFormPl.RESWORD1[6]   = 0 ))
     and ((SaldTune.WKAU[6] = 0) or (SaldTune.WKAU[6] <> 0 and spFormPl.RESWORD1[7]   = 0 ))
